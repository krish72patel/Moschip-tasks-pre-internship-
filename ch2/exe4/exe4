#include<stdio.h>
#include<string.h>
#define MAX 100
void shrink (char x1[],char x2[]);

int main()
{
        int i;
        char c;
        char s1[MAX];
        char s2[MAX];
        printf("enter string1\n");
        for (i=0;(c=getchar())!='\n'&& i<MAX;i++)
        {
                s1[i]=c;
        }
        s1[++i]='\0';
        printf("enter string2\n");
        for(i=0;(c=getchar())!='\n'&&i<MAX;i++)
        {
                s2[i]=c;
        }
        s2[++i]='\0';
        shrink(s1,s2);
        printf("non repeating characters in string \n%s \n",s1);
}

void shrink(char x1[],char x2[])
{
        char temp[MAX]={0};
        int i,j,k,count;
        i=j=k=0;
        for(i=0;x1[i]!='\0';i++)
        {
                count=0;
                for(j=0;x2[j]!='\0';j++)//check repeating character
                {
                        if(x1[i]==x2[j])
                        {
                                ++count;
                        }
                }
                if(count==0)//store non repeating char temperory
                {
                        temp[k]=x1[i];
                        ++k;
                }

        }
                strcpy(x1,temp);
}


